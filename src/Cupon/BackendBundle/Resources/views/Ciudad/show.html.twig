{% extends '::backend.html.twig' %}

{% block article %}
    <h1>Ciudad</h1>

    <table class="record_properties">
        <tbody>

            <tr>
                <th>Nombre</th>
                <td>{{ entity.nombre }}</td>
            </tr>
            <tr>
                <th>Slug</th>
                <td>{{ entity.slug }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('backend_ciudad') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('backend_ciudad_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
